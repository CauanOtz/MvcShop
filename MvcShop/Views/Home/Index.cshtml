@model IEnumerable<MvcShop.Models.Category>
@using System.Linq
@{
    ViewData["Title"] = "Home";
}

<div class="container-fluid px-0">
    <div id="heroCarousel" class="carousel slide" data-bs-ride="carousel">
        <div class="carousel-indicators">
            <button type="button" data-bs-target="#heroCarousel" data-bs-slide-to="0" class="active"></button>
            <button type="button" data-bs-target="#heroCarousel" data-bs-slide-to="1"></button>
            <button type="button" data-bs-target="#heroCarousel" data-bs-slide-to="2"></button>
        </div>
        <div class="carousel-inner rounded-0">
            <div class="carousel-item active">
                <img src="data:image/jpeg;base64," class="d-block w-100" alt="Promo 1">
                <div class="carousel-caption d-none d-md-block">
                    <h2 class="display-5 fw-bold">Coleção Verão</h2>
                    <p class="lead">Até 50% OFF em selecionados</p>
                    <a href="/Products" class="btn btn-primary btn-lg">Compre Agora</a>
                </div>
            </div>
            <div class="carousel-item">
                <img src="data:image/jpeg;base64,
                <div class="carousel-caption d-none d-md-block">
                    <h2 class="display-5 fw-bold">Novidades</h2>
                    <p class="lead">Os lançamentos mais esperados</p>
                    <a href="/Products" class="btn btn-primary btn-lg">Descubra</a>
                </div>
            </div>
            <div class="carousel-item">
                <img src="https://img.cdndsgni.com/preview/11005492.jpg" class="d-block w-100" alt="Promo 3">
                <div class="carousel-caption d-none d-md-block">
                    <h2 class="display-5 fw-bold">Frete Grátis</h2>
                    <p class="lead">Em compras acima de R$199</p>
                    <a href="/Products" class="btn btn-primary btn-lg">Aproveite</a>
                </div>
            </div>
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#heroCarousel" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Anterior</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#heroCarousel" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Próximo</span>
        </button>
    </div>
</div>

<div class="container mt-5">
    <h2 class="text-center mb-4 fw-bold">Nossas Categorias</h2>
    <div class="row g-4">
        @foreach(var cat in Model.Take(3))
        {
            var image = cat.Products?.FirstOrDefault()?.ImageUrl ?? "https://via.placeholder.com/640x360?text=Categoria";
            <div class="col-md-4">
                <div class="card h-100 border-0 shadow rounded-4 overflow-hidden category-card">
                    <img src="@image" class="card-img-top" alt="@cat.Name">
                    <div class="card-body text-center">
                        <h5 class="card-title">@cat.Name</h5>
                        <p class="card-text text-muted">@((cat.Products?.Count ?? 0)) produtos</p>
                        <a href="/Products?category=@cat.Id" class="btn btn-outline-primary">Explorar</a>
                    </div>
                </div>
            </div>
        }
    </div>
</div>

<div class="container mt-5 py-5">
    <h2 class="text-center mb-4 fw-bold">Destaques</h2>
    <div class="row row-cols-1 row-cols-md-3 row-cols-lg-4 g-4">
        @foreach(var p in Model.SelectMany(c => c.Products ?? Enumerable.Empty<MvcShop.Models.Product>()).Take(8))
        {
            <div class="col">
                <div class="card h-100 border-0 shadow-sm rounded-4 overflow-hidden position-relative">
                    <span class="badge bg-danger position-absolute m-2">-30%</span>
                    <img src="@(p.ImageUrl ?? "https://via.placeholder.com/320x240?text=Produto")" class="card-img-top" alt="@p.Name">
                    <div class="card-body">
                        <h6 class="card-title mb-1">@p.Name</h6>
                        <p class="text-muted small mb-2">@p.Category?.Name</p>
                        <div class="d-flex justify-content-between align-items-center">
                            <div>
                                <span class="text-decoration-line-through text-muted me-2">@((p.Price * 1.2m).ToString("C"))</span>
                                <span class="fw-bold text-primary">@p.Price.ToString("C")</span>
                            </div>
                            <button class="btn btn-sm btn-outline-secondary rounded-circle" title="Favoritar"><i class="fa-regular fa-heart"></i></button>
                        </div>
                    </div>
                    <div class="card-footer bg-transparent border-0">
                        <form method="post" action="/Cart/Add" class="d-flex">
                            <input type="hidden" name="productId" value="@p.Id" />
                            <input type="hidden" name="quantity" value="1" />
                            <button class="btn btn-primary w-100" type="submit"><i class="fa-solid fa-cart-plus me-1"></i> Adicionar ao Carrinho</button>
                        </form>
                    </div>
                    <a href="/Products/Details/@p.Id" class="stretched-link"></a>
                </div>
            </div>
        }
    </div>
</div>

<div class="bg-primary text-white py-5 mt-5">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-6 text-center">
                <h2 class="fw-bold mb-3">Receba Ofertas Exclusivas</h2>
                <p class="mb-4">Assine nossa newsletter e ganhe 10% OFF na primeira compra</p>
                <form method="post" action="/Newsletter/Subscribe" class="d-flex gap-2 justify-content-center">
                    <input type="email" name="email" class="form-control w-75" placeholder="Seu melhor e-mail" required />
                    <button class="btn btn-light text-primary fw-bold" type="submit">Assinar</button>
                </form>
            </div>
        </div>
    </div>
</div>

<style>
    :root { --primary: #6f42c1; --secondary: #ff6b6b; }
    .carousel-caption { text-align: left; left: 8%; right: auto; bottom: 20%; }
    .category-card img { height: 220px; object-fit: cover; }
    .card .card-img-top { height: 220px; object-fit: cover; }
    .category-card .card-body { padding-top: 1rem; padding-bottom: 1rem; }
    @@media (max-width: 575.98px) { .carousel-caption { display:none; } }
</style>
